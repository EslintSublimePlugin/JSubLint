{
  "plugins": [
    "angular"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "no-shadow-restricted-names": "off",
    "angular/value-name": 0,
    "no-inner-declarations": "error",
    "no-extend-native": "off",
    "angular/json-functions": 2,
    "angular/module-setter": 2,
    "space-in-parens": "off",
    "angular/module-name": 0,
    "no-plusplus": "off",
    "radix": "off",
    "accessor-pairs": "off",
    "require-jsdoc": "off",
    "multiline-ternary": "off",
    "no-iterator": "off",
    "no-sparse-arrays": "error",
    "no-prototype-builtins": "off",
    "no-redeclare": "error",
    "valid-jsdoc": "off",
    "no-lone-blocks": "off",
    "block-scoped-var": "off",
    "no-nested-ternary": "off",
    "array-callback-return": "off",
    "no-empty": "error",
    "angular/constant-name": 0,
    "operator-linebreak": "off",
    "no-alert": "off",
    "func-names": "off",
    "angular/no-jquery-angularelement": 2,
    "no-loop-func": "off",
    "no-constant-condition": "error",
    "func-name-matching": "off",
    "no-unused-expressions": "off",
    "angular/typecheck-object": 2,
    "object-property-newline": "off",
    "no-whitespace-before-property": "off",
    "no-unused-vars": "error",
    "nonblock-statement-body-position": "off",
    "no-implicit-globals": "off",
    "no-floating-decimal": "off",
    "vars-on-top": "off",
    "no-new": "off",
    "quotes": "off",
    "angular/typecheck-date": 2,
    "space-infix-ops": "off",
    "consistent-this": "off",
    "no-bitwise": "off",
    "dot-location": "off",
    "no-continue": "off",
    "no-eq-null": "off",
    "angular/provider-name": 0,
    "no-multi-spaces": "off",
    "require-await": "off",
    "no-throw-literal": "off",
    "no-underscore-dangle": "off",
    "id-blacklist": "off",
    "space-unary-ops": "off",
    "max-statements": "off",
    "angular/function-type": 0,
    "space-before-function-paren": "off",
    "no-negated-condition": "off",
    "no-param-reassign": "off",
    "angular/directive-restrict": 0,
    "no-array-constructor": "off",
    "no-return-assign": "off",
    "block-spacing": "off",
    "one-var-declaration-per-line": "off",
    "no-fallthrough": "error",
    "prefer-promise-reject-errors": "off",
    "lines-around-directive": "off",
    "no-restricted-globals": "off",
    "no-unsafe-negation": "off",
    "angular/component-name": 0,
    "no-mixed-operators": "off",
    "max-len": "off",
    "no-extra-boolean-cast": "error",
    "eol-last": "off",
    "no-labels": "off",
    "angular/no-cookiestore": 2,
    "quote-props": "off",
    "angular/one-dependency-per-line": 0,
    "angular/foreach": 0,
    "newline-before-return": "off",
    "wrap-iife": "off",
    "class-methods-use-this": "off",
    "no-regex-spaces": "error",
    "new-cap": "off",
    "angular/no-service-method": 2,
    "default-case": "off",
    "no-unmodified-loop-condition": "off",
    "angular/rest-service": 0,
    "angular/directive-name": 0,
    "no-extra-bind": "off",
    "angular/window-service": 2,
    "no-with": "off",
    "no-console": "error",
    "no-undef": "error",
    "no-multiple-empty-lines": "off",
    "no-useless-escape": "off",
    "sort-vars": "off",
    "brace-style": "off",
    "no-proto": "off",
    "angular/controller-name": [
      2,
      "/[A-Z].*Controller$/"
    ],
    "comma-dangle": "off",
    "angular/module-getter": 2,
    "no-useless-return": "off",
    "no-use-before-define": "off",
    "no-implicit-coercion": "off",
    "wrap-regex": "off",
    "angular/typecheck-function": 2,
    "no-undefined": "off",
    "no-new-wrappers": "off",
    "angular/filter-name": 0,
    "no-unreachable": "error",
    "angular/module-dependency-order": 0,
    "object-curly-spacing": "off",
    "no-empty-pattern": "error",
    "no-tabs": "off",
    "max-params": "off",
    "angular/controller-as-vm": [
      2,
      "vm"
    ],
    "angular/empty-controller": 0,
    "curly": "off",
    "no-script-url": "off",
    "angular/no-http-callback": 0,
    "comma-style": "off",
    "no-empty-function": "off",
    "no-new-func": "off",
    "camelcase": "off",
    "no-unused-labels": "error",
    "no-mixed-spaces-and-tabs": "error",
    "no-global-assign": "error",
    "array-bracket-spacing": "off",
    "consistent-return": "off",
    "linebreak-style": "off",
    "angular/controller-as-route": 2,
    "func-call-spacing": "off",
    "one-var": "off",
    "angular/interval-service": 2,
    "angular/no-services": [
      2,
      [
        "$http",
        "$resource",
        "Restangular"
      ]
    ],
    "no-void": "off",
    "max-statements-per-line": "off",
    "angular/deferred": 0,
    "no-magic-numbers": "off",
    "no-div-regex": "off",
    "newline-after-var": "off",
    "guard-for-in": "off",
    "no-self-assign": "error",
    "angular/no-angular-mock": 0,
    "padded-blocks": "off",
    "no-trailing-spaces": "off",
    "no-catch-shadow": "off",
    "no-undef-init": "off",
    "angular/typecheck-number": 2,
    "no-multi-assign": "off",
    "no-lonely-if": "off",
    "angular/no-private-call": 2,
    "no-multi-str": "off",
    "angular/component-limit": [
      0,
      1
    ],
    "id-length": "off",
    "max-nested-callbacks": "off",
    "newline-per-chained-call": "off",
    "valid-typeof": "error",
    "angular/service-name": 2,
    "sort-keys": "off",
    "no-new-object": "off",
    "space-before-blocks": "off",
    "angular/log": 2,
    "complexity": "off",
    "no-debugger": "error",
    "no-delete-var": "error",
    "spaced-comment": "off",
    "no-dupe-keys": "error",
    "angular/angularelement": 1,
    "no-extra-label": "off",
    "no-unexpected-multiline": "error",
    "max-depth": "off",
    "template-tag-spacing": "off",
    "angular/on-watch": 2,
    "angular/watchers-execution": [
      0,
      "$digest"
    ],
    "no-useless-concat": "off",
    "unicode-bom": "off",
    "id-match": "off",
    "no-useless-call": "off",
    "no-octal-escape": "off",
    "no-shadow": "off",
    "no-duplicate-case": "error",
    "lines-around-comment": "off",
    "angular/no-directive-replace": 0,
    "no-compare-neg-zero": "off",
    "angular/factory-name": 0,
    "angular/document-service": 2,
    "semi": "off",
    "no-func-assign": "error",
    "dot-notation": "off",
    "angular/no-inline-template": 0,
    "eqeqeq": "off",
    "angular/on-destroy": 0,
    "yoda": "off",
    "no-case-declarations": "error",
    "no-implied-eval": "off",
    "semi-spacing": "off",
    "line-comment-position": "off",
    "no-label-var": "off",
    "comma-spacing": "off",
    "no-await-in-loop": "off",
    "no-return-await": "off",
    "angular/controller-as": 2,
    "no-cond-assign": "error",
    "angular/file-name": 0,
    "no-dupe-args": "error",
    "indent": "off",
    "keyword-spacing": "off",
    "no-ex-assign": "error",
    "no-invalid-regexp": "error",
    "angular/timeout-service": 2,
    "angular/definedundefined": 2,
    "angular/di-order": [
      0,
      true
    ],
    "no-empty-character-class": "error",
    "no-unsafe-finally": "error",
    "no-extra-parens": "off",
    "no-eval": "off",
    "key-spacing": "off",
    "no-restricted-properties": "off",
    "no-invalid-this": "off",
    "angular/dumb-inject": 0,
    "no-restricted-syntax": "off",
    "no-warning-comments": "off",
    "angular/typecheck-array": 2,
    "no-else-return": "off",
    "no-control-regex": "error",
    "capitalized-comments": "off",
    "init-declarations": "off",
    "func-style": "off",
    "angular/typecheck-string": 2,
    "no-irregular-whitespace": "error",
    "jsx-quotes": "off",
    "no-octal": "error",
    "object-curly-newline": "off",
    "max-lines": "off",
    "no-obj-calls": "error",
    "angular/no-controller": 0,
    "no-self-compare": "off",
    "no-sequences": "off",
    "no-ternary": "off",
    "computed-property-spacing": "off",
    "angular/di-unused": 0,
    "use-isnan": "error",
    "angular/di": [
      0,
      "function"
    ],
    "operator-assignment": "off",
    "angular/prefer-component": 0,
    "angular/no-run-logic": 0,
    "no-inline-comments": "off",
    "no-template-curly-in-string": "off",
    "no-extra-semi": "error",
    "no-caller": "off",
    "new-parens": "off",
    "no-unneeded-ternary": "off"
  }
}